<script>

  $(".clickable1").click(function () {

    $(this).parent().addClass("active");
    $(".clickable2").parent().removeClass("active");
    $(document).find(".panel").removeClass('flipped');
    $(document).find(".panel").addClass('panel-default');
    $(document).find(".panel").addClass('panel-inverted');



  });

  $(".clickable2").click(function () {

    $(this).parent().addClass("active");
    $(".clickable1").parent().removeClass("active");
    $(document).find(".panel").removeClass('panel-inverted');
    $(document).find(".panel").addClass('flipped');
    $(document).find(".panel").addClass('panel-default');

  })

  $("#FormValidate").validate({
    rules: {
      firstname: {
        required: true,

      },
      lastname: {
        required: true,

      },
      email: {
        required: true,
        email: true
      },
      email_confirmation: {
        required: true,
        email: true,
        equalTo: '[name="email"]'
      },
      password: {
        required: true,

      },
      password_confirmation: {
        required: true,
        equalTo: '[name="password"]'

      },


    },
    //messages: {


    //},

  });


  $("#btn_Registration").click(function () {

    $("#Errorappend").html("");

    if ($("#FormValidate").valid()) {

      var response = grecaptcha.getResponse();
      if (response.length == 0) {
  
        $("#Errorappend").append(`<h5 style="color:red">Please verify reCAPTCHA</h5>`)
      }
      else {
        AddRegistration();
      }

    }
  })

  function AddRegistration() {
    $("#Errorappend").html("");
    $("#Loading_Show").show();
    $.ajax({
      url: "/Account/CreateRegister",
      data: JSON.stringify({
        UserName: $("#firstname").val() + " " + $("#lastname").val(),
        Password: $("#password").val(),
        Email: $("#email").val(),

      }),
      type: "POST",
      contentType: "application/json;charset=utf-8",
      dataType: "json",
      success: function (result) {
        $("#Loading_Show").hide();
        grecaptcha.reset();
      
        if (result.success) {
          window.location.href ="/Account/Confirmation"
        } else {

          if (result.message.length > 1) {

            $("#Errorappend").append(`<div> <h6 style="color:red">${result.message[0]}</h6><h6 style="color:red">${result.message[1]}</h6></div>`);
          } else {

            $("#Errorappend").append(`<div><h6 style="color:red">${result.message[0]}</h6></div>`);
          }
 
        }
      },
      error: function (errormessage) {
        $("#Loading_Show").hide();
        alert(errormessage.responseText);
      }
    });
  }

</script>
